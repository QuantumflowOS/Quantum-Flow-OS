{
  "name": "@quantum-flow/ethical-ontology",
  "version": "14.0.0",
  "description": "Self-constraining ethical framework with built-in governance and observer protection",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:dev": "tsc --sourceMap",
    "build:prod": "tsc --declaration",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ethical": "jest --testPathPattern=ethical",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "start": "node dist/index.js",
    "start:ethical": "node dist/index.js --ethical-mode=strict",
    "validate": "npm run lint && npm run test",
    "clean": "rm -rf dist coverage",
    "prepare": "npm run build"
  },
  "keywords": [
    "ethical-ai",
    "self-constraining",
    "observer-protection",
    "reversibility",
    "governance",
    "typescript",
    "framework"
  ],
  "author": "Quantum Flow Ethics Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/quantum-flow/quantum-flow-os.git"
  },
  "bugs": {
    "url": "https://github.com/quantum-flow/quantum-flow-os/issues"
  },
  "homepage": "https://github.com/quantum-flow/quantum-flow-os#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "uuid": "^9.0.0",
    "eventemitter3": "^5.0.1",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.2.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/tests"
    ],
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/?(*.)+(spec|test).ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/**/index.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}
